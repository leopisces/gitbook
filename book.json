{
	"title": "",
	"description": "Welcome to my learning world!",
	"author": "leopisces",
	"output.name": "site",
	"language": "zh-hans",
	"gitbook": "3.2.3",
	"root": ".",
	"plugins": [
		"-lunr",
		"-search",
		"-sharing",
		"-default-theme",
		"search-pro",
		"theme-comscore",
		"-splitter",
		"tbfed-pagefooter",
		"expandable-chapters",
		"hide-element",
		"-custom-favicon",
		"-highlight",
		"prism",
		"prism-themes",
		"-code",
		"sectionx",
		"flexible-alerts",
		"ancre-navigation",
		"chapter-fold",
		"todo",
		"-edit-link",
		"-favicon",
		"-theme-fexa",
		"lightbox",
		"-pageview-count",
		"simple-mind-map"
	],
	"variables": {
		"themeFexa": {
			"nav": [{
					"url": "https://github.com/",
					"target": "_blank",
					"name": "GitHub"
				},
				{
					"url": "https://www.baidu.com/",
					"target": "_blank",
					"name": "百度"
				},
				{
					"url": "https://www.google.com/",
					"target": "_blank",
					"name": "谷歌"
				}
			]
		}
	},
	"pluginsConfig": {
		"theme-fexa": {
			"search-placeholder": "输入关键字搜索",
			"logo": "./logo.png",
			"favicon": "./favicon.ico"
		},
		"favicon": "asset/csdn.ico",
		"edit-link": {
			"base": "//github.com/yulilong/book/edit/master",
			"label": "编辑此页面"
		},
		"tbfed-pagefooter": {
			"copyright": "Copyright © 2022 浙ICP备2022008908号",
			"modify_label": "该文件修订时间：",
			"modify_format": "YYYY-MM-DD HH:mm:ss"
		},

		"hide-element": {
			"elements": [".gitbook-link"]
		},

		"pdf": {
			"pageNumbers": true,
			"fontFamily": "Arial",
			"fontSize": 12,
			"paperSize": "a4",
			"margin": {
				"right": 62,
				"left": 62,
				"top": 56,
				"bottom": 56
			}
		},
		"page-toc-button": {
			"maxTocDepth": 2,
			"minTocSize": 2
		},
		"prism": {
			"css": [
				"prism-themes/themes/prism-atom-dark.css"
			]
		},
		"sectionx": {
			"tag": "b"
		}
	},

	"styles": {
		"website": "styles/website.css",
		"ebook": "styles/ebook.css",
		"pdf": "styles/pdf.css",
		"mobi": "styles/mobi.css",
		"epub": "styles/epub.css"
	},

	"simple-mind-map": {
		"type": "markdown",
		"preset": "colorful",
		"linkShape": "diagonal",
		"autoFit": true,
		"style": {
			"height":  "750px"
		}
	}
}